//Higher order function
function add(num1, num2) {
return num1 + num2;
}
 
function subtract(num1, num2) {
return num1 - num2;
}
 
function multiply(num1, num2) {
return num1 * num2;
}
 
function divide(num1, num2) {
return num1 / num2;
}
 
function calculator(num1, num2, operator) {
return operator(num1, num2);
}

caculator (1, 2, add)

//event listener
function eventListener(event, callBack) {
    var eventHappened = {
        eventName: "click",
        eventKey: "c"
    }
        if (event === eventHappened.eventName) {
            //do something
            callBack(eventHappened)
        }
}

function call(x) {
    console.log(x)
}

debugger;
eventListener("click", call)
